var userNumber = 0;
userNumber = prompt("Digite um número para verificar se é positivo ou negativo: ");
if(userNumber > 0){
    alert(`Seu número ${userNumber} é um número positivo!`);
}
if(userNumber < 0){
    alert(`Seu número ${userNumber} é um número negativo!`);
} 
